<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.valdosta.workoutapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnduranceActivity" />
        <activity android:name=".StrengthActivity" />
        <activity android:name=".FlexibiltyActivity" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MyWorkoutsActivity" />
        <activity android:name=".RegionActivity" />
        <activity android:name=".DisplayExercise" />
        <activity android:name=".AddToWorkoutsActivity"
            android:theme="@style/AppTheme.CustomTheme"/>
        <activity android:name=".DeleteFromWorkoutsActivity"
            android:theme="@style/AppTheme.CustomTheme"/>
    </application>

</manifest>